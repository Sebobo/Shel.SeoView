prototype(Shel.SeoView:ExtractedKeywords) < prototype(Neos.Fusion:Tag) {
    tagName = 'section'
    attributes.class = 'seo-view__analyzed-keywords'

    content = Neos.Fusion:Array {
        title = Neos.Fusion:Tag {
            tagName = 'h2'
            content = ${Translation.translateById('sections.extractedKeywords.title', 'Shel.SeoView', 'Main')}
        }

        keywords = Neos.Fusion:Tag {
            tagName = 'table'
            content = Neos.Fusion:Collection {
                aggregationDefinition = Neos.Fusion:RawArray {
                    terms = Neos.Fusion:RawArray {
                        field = '__fulltext.text'
                        size = 100
                    }
                }

                collection = ${Search.query(documentNode).nodeType('TYPO3.Neos:Document').limit(0).aggregation("keywords", this.aggregationDefinition).execute().getAggregations().keywords.buckets}
                itemRenderer = Neos.Fusion:Tag {
                    tagName = 'tr'
                    content = ${'<td>' + item.key + '</td><td>' + item.doc_count + '</td>'}
                }
            }
        }
    }
}
